
  cmpq $new_node, (root)
  jne bad_exit

  movq $new_node, %rbx
  cmpq $5, (%rbx)
  jne bad_exit
  cmpq $0, 8(%rbx)
  jne bad_exit
  cmpq $0, 16(%rbx)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
root:
        .zero   32
new_node:
        .quad   5
        .quad   0
        .quad   0
