
  cmpq $A, (root)
  jne bad_exit

  movq $A, %rbx
  cmpq $20, (%rbx)
  jne bad_exit
  cmpq $B, 8(%rbx)
  jne bad_exit
  cmpq $C, 16(%rbx)
  jne bad_exit

  movq $B, %rbx
  cmpq $-4, (%rbx)
  jne bad_exit
  cmpq $0, 8(%rbx)
  jne bad_exit
  cmpq $D, 16(%rbx)
  jne bad_exit

  movq $C, %rbx
  cmpq $26, (%rbx)
  jne bad_exit
  cmpq $0, 8(%rbx)
  jne bad_exit
  cmpq $0, 16(%rbx)
  jne bad_exit

  movq $D, %rbx
  cmpq $-3, (%rbx)
  jne bad_exit
  cmpq $0, 8(%rbx)
  jne bad_exit
  cmpq $new_node, 16(%rbx)
  jne bad_exit

  movq $new_node, %rbx
  cmpq $0, (%rbx)
  jne bad_exit
  cmpq $0, 8(%rbx)
  jne bad_exit
  cmpq $0, 16(%rbx)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
root:
        .quad   A
A:
        .quad   20
        .quad   B
        .quad   C
B:
        .quad   -4
        .quad   0
        .quad   D
C:
        .quad   26
        .quad   0
        .quad   0
D:
        .quad   -3
        .quad   0
        .quad   0

new_node:
        .quad   0
        .quad   0
        .quad   0
